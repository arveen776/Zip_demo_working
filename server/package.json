{
  "name": "orderbook-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "prisma generate",
    "start": "node index.js",
    "dev": "node startup.js && node index.js",
    "migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force && prisma migrate deploy",
    "db:deploy": "prisma migrate resolve --applied 20250730000136_add_customer_email && prisma migrate deploy",
    "db:init": "node startup.js",
    "fix-migrations": "node fix-migrations.js"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "moment-timezone": "^0.6.0",
    "nodemailer": "^7.0.5"
  },
  "devDependencies": {
    "ethereal-email": "^1.0.0",
    "prisma": "^6.12.0"
  }
}
